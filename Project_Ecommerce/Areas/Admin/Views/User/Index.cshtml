<div class="row">
    <div class="col-9">
        <h2 class="text-primary">User List</h2>
    </div>
    <div class="col-3">
        <a asp-area="Identity" asp-page="/Account/Register" class="btn btn-primary form-control">
        <i class="fas fa-plus"></i>&nbsp; Add New User    
        </a>
    </div>
</div>
<div class="container rounded border p-2 m-2">
    <table id="tbldata" class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Phone Number</th>
                <th>Company</th>
                <th>Roles</th>
                <th>Actions</th>
            </tr>
        </thead>
    </table>
</div>

@section scripts
{
    <script src="~/js/User.js"></script>

}